{
	"extends": "eslint:recommended",
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true,
		"jquery": true
	},
	"rules": {
		"no-debugger": 0,
		"no-console": 0,
		"new-cap": 0,
		"strict": 0,
		"no-underscore-dangle": 0,
		"no-use-before-define": 0,
		"eol-last": 0,
		"no-class-assign": 0,
		"quotes": [2, "single"],
		"indent": [2, "tab", {
			"SwitchCase": 1,
			"VariableDeclarator": {
				"var": 2,
				"let": 2,
				"const": 3
			}
		}],
		"no-mixed-spaces-and-tabs": "error",
		"no-unused-vars": [
			0,
			{
				"vars": "all",
				"args": "after-used"
			}
		],
		"no-undef": 0,
		"jsx-quotes": [
			2,
			"prefer-double"
		]
	}
}
